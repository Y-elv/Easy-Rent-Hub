{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc",
    "dev": "nodemon",
    "ts:dev": "ts-node-dev --respawn --rs --watch src src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "db:migrate": "npx sequelize-cli db:migrate --config=config/config.ts",
    "db:rollback": "npx sequelize-cli db:migrate:undo --config=config/config.ts",
    "db:seed": "npx sequelize-cli db:seed:all --config=config/config.ts",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all --config=config/config.ts"
  },
